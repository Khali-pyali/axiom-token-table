{
    "version": 2,
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.ts"
        }
    ],
    "env": {
        "PORT": "3001",
        "WS_PORT": "3002",
        "CORS_ORIGINS": ""
    }
}